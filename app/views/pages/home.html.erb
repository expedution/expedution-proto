

    <!-- Navbar
    ================================================== -->

<%= render "shared/navbar" %>

    <!-- Hero Section
    ================================================== -->

    <div class="banner img-responsive" style="background-image: linear-gradient(to right, rgba(224, 93, 73,0.6) 0%, rgba(153, 107, 130,0.6) 40%, rgba(87, 135, 198,0.6) 80%, rgba(62, 151, 232,0.6) 100%), url('https://static.pexels.com/photos/527/man-person-people-train.jpg'); height: 100vh;">
      <div class="banner-content">
        <h1 class="css-typing">Expedution, move work forward</h1>
        <p>Expedution is the easiest way to craft your learning expeditions, innovation tours...</p>
        <%= link_to "CRAFT YOUR ADVENTURE", new_user_session_path, class: "waves-effect waves-light waves-purple btn-large green grey-text white-text", style: "text-decoration: none; font-weight: 600;" %>
      </div>
    </div>


    <video autoplay  poster="" id="bgvid" style="background-image: linear-gradient(to right, rgba(224, 93, 73,0.8) 0%, rgba(153, 107, 130,0.6) 40%, rgba(87, 135, 198,0.8) 80%, rgba(62, 151, 232,0.6) 100%), url(''); height: 75vh;" loop>
    </video>

    <!-- Intro
    ================================================== -->

    <section class="section-intro text-center valign-wrapper">
      <div class="container">
        <h2>powerful management tool for impressing your clients</h2>
        <div class="row">
          <div class="col-md-12">
            <div class="container text-center">
              <div class="col-md-12">
                <img src="http://i.imgur.com/9ZwlxtW.png" alt="iPad mock" class="img-responsive">
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Features
    ================================================== -->

    <section class="section-features valign-wrapper">
      <div class="container">
        <div class="row">
          <div class="col-md-6">
            <div class="col-md-12">
              <div class="card-feature materialboxed">
                <div class="card-image hoverable z-depth-1">
                  <img src="http://i.imgur.com/A24gdAM.png" alt="" class="img-responsive">
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <div class="col-md-12">
              <div class="card-feature">
                <div class="card-block">
                  <span class="icon-thunderbolt display-1"></span>
                  <div class="title-feature">
                    <h2 style="font-weight: bold;">See how it works.</h2>
                  </div>
                  <p class="card-text">Start to organise your own Learning Expeditions and save countless hours</p>
                  <%= link_to "TRY IT TODAY", new_user_session_path, class: "waves-effect waves-light waves-purple btn-large green grey-text white-text", style: "text-decoration: none; font-weight: 600;" %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>



    <!-- Extra features
    ================================================== -->

    <section class="section-extra-features valign-wrapper">
      <div class="container">
        <h3 class="text-right white-text">One tool to rule them all.</h1>
        <div class="row padded">
          <div class="col s12 m4 l4">
            <div class="card hoverable">
              <div class="card-image waves-effect waves-block waves-light">
                <img class="activator" src="https://static.pexels.com/photos/2769/hands-way-guide-tourist.jpg">
                <span class="card-title activator">Source</span>
              </div>
              <div class="card-content">
                <p>Easily source thousands of companies thanks to our powerful search engine.</p>
              </div>
              <div class="card-reveal">
                    <span class="card-title grey-text text-darken-4">Card Title<i class="material-icons right">close</i></span>
                    <p>Here is some more information about this product that is only revealed once clicked on.</p>
              </div>
            </div>
          </div>
          <div class="col s12 m4 l4">
            <div class="card hoverable">
              <div class="card-image waves-effect waves-block waves-light">
                <img class="activator" src="https://static.pexels.com/photos/3855/wood-architect-table-work.jpeg">
                <span class="card-title activator">Craft</span>
              </div>
              <div class="card-content">
                <p>Use our software to craft your next learning expedition. No more spreadsheets!</p>
              </div>
              <div class="card-reveal">
                    <span class="card-title grey-text text-darken-4">Card Title<i class="material-icons right">close</i></span>
                    <p>Here is some more information about this product that is only revealed once clicked on.</p>
              </div>
            </div>
          </div>
          <div class="col s12 m4 l4">
            <div class="card hoverable">
              <div class="card-image waves-effect waves-block waves-light">
                <img class="activator" src="https://static.pexels.com/photos/70292/pexels-photo-70292.jpeg">
                <span class="card-title activator">Communicate</span>
              </div>
              <div class="card-content">
                <p>Easily manage communication with your participants and receive instant feedbacks</p>
              </div>
              <div class="card-reveal">
                    <span class="card-title grey-text text-darken-4">Card Title<i class="material-icons right">close</i></span>
                    <p>Here is some more information about this product that is only revealed once clicked on.</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

